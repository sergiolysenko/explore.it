{
  "name": "explore.it",
  "version": "1.0.0",
  "description": "standart package width gulp",
  "main": "index.js",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiolysenko/explore.it.git"
  },
  "keywords": [
    "italy",
    "explore",
    "travel"
  ],
  "author": "Sergei Lysenko",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "9.0.0",
    "browser-sync": "2.26.13",
    "del": "6.0.0",
    "gulp": "4.0.2",
    "gulp-csso": "4.0.1",
    "gulp-imagemin": "7.1.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.0",
    "gulp-posthtml": "3.0.5",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-webp": "4.0.1",
    "posthtml-include": "1.6.0",
    "stylelint": "13.8.0",
    "stylelint-config-htmlacademy": "0.1.4"
  },
  "bugs": {
    "url": "https://github.com/sergiolysenko/explore.it/issues"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "homepage": "https://github.com/sergiolysenko/explore.it#readme"
